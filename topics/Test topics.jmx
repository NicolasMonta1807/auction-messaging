          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">import org.zeromq.ZMQ
import org.zeromq.ZContext

// Crea un contexto
ZContext context = new ZContext()

// Crea un socket PUB
ZMQ.Socket socket = context.createSocket(ZMQ.PUB)
socket.bind(&quot;tcp://localhost:5555&quot;) // Cambié &apos;connect&apos; por &apos;bind&apos;

// Envía un mensaje
int i = 1 // Asume que tienes un valor para &apos;i&apos;
String message = &quot;{ \&quot;bid\&quot;: \&quot;&quot; + i + &quot;\&quot;, \&quot;timestamp\&quot;: &quot; + System.currentTimeMillis() + &quot; }&quot;
socket.send(message.getBytes(ZMQ.CHARSET))

// Limpia recursos
socket.close()
context.close()
</stringProp>
